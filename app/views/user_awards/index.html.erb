<p id="notice"><%= notice %></p>

<h1>User Awards</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_awards.each do |user_award| %>
      <tr>
        <td><%= user_award.year %></td>
        <td><%= user_award.user_id %></td>
        <td><%= link_to 'Show', user_award %></td>
        <td><%= link_to 'Edit', edit_user_award_path(user_award) %></td>
        <td><%= link_to 'Destroy', user_award, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Award', new_user_award_path %>
